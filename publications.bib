  annotation = {preprintdoi:10.26434/chemrxiv-2024-b7kv6},
  file = {/Users/es/Zotero/storage/H7892Z58/Lukas et al. - 2024 - Graphitic Carbon Nitride as a Photocatalyst for Decarboxylative C(sp2 )−C(sp3.pdf}
