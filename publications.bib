  annotation = {preprintdoi:10.26434/chemrxiv-2023-4g3xz},
  file = {/Users/es/Zotero/storage/67AA29LV/Wan et al. - 2024 - Rapid and scalable photocatalytic C(sp2)–C(sp3) Suzuki−Miyaura cross-coupling of aryl bromides with.pdf}
}

