@article{schuurmansInteractionLightGas2025,
  title = {Interaction of Light with Gas--Liquid Interfaces: Influence on Photon Absorption in Continuous-Flow Photoreactors},
  shorttitle = {Interaction of Light with Gas--Liquid Interfaces},
  author = {Schuurmans, Jasper H. A. and Zondag, Stefan D. A. and Chaudhuri, Arnab and Claros, Miguel and Van Der Schaaf, John and No{\"e}l, Timothy},
