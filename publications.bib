  annotation = {preprintdoi:10.26434/chemrxiv-2024-sz6ng)},
  file = {/Users/es/Zotero/storage/RSCGRHAN/Schuurmans et al. - 2024 - Light-assisted carbon dioxide reduction in an automated photoreactor system coupled to carbonylation.pdf}
